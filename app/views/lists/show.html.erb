<h1><%= @list.name %></h1>

<ul>
  <% @list.movies.each do |movie| %>
    <li>
      <%= movie.title %>
      <% bookmark = Bookmark.find_by(list: @list, movie: movie) %>
    </li>
    <% end %>
</ul>
      
      <%= link_to 'Back to Lists', lists_path %>
      <%= link_to 'Add a Bookmark', new_list_bookmark_path(@list), class: 'btn btn-primary'  %>